  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "if not df.empty and 'product_clean' in df.columns and 'market_clean' in df.columns and 'price' in df.columns:\n",
    "    # S√©lection des produits et march√©s les plus fr√©quents\n",
    "    top_products = df['product_clean'].value_counts().head(10).index\n",
    "    top_markets = df['market_clean'].value_counts().head(8).index\n",
    "    \n",
    "    # Filtre des donn√©es\n",
    "    filtered_df = df[\n",
    "        (df['product_clean'].isin(top_products)) & \n",
    "        (df['market_clean'].isin(top_markets))\n",
    "    ]\n",
    "    \n",
    "    # Pivot table pour la heatmap\n",
    "    pivot_data = filtered_df.pivot_table(\n",
    "        values='price', \n",
    "        index='product_clean', \n",
    "        columns='market_clean', \n",
    "        aggfunc='mean'\n",
    "    )\n",
    "    \n",
    "    # Visualisation avec matplotlib\n",
    "    plt.figure(figsize=(12, 8))\n",
    "    sns.heatmap(pivot_data, annot=True, fmt='.2f', cmap='YlOrRd', \n",
    "                cbar_kws={'label': 'Prix moyen (‚Ç¨)'})\n",
    "    plt.title('Heatmap des prix moyens par produit et march√©')\n",
    "    plt.xlabel('March√©')\n",
    "    plt.ylabel('Produit')\n",
    "    plt.tight_layout()\n",
    "    plt.show()\n",
    "    \n",
    "    print(\"üìä Matrice des prix moyens:\")\n",
    "    display(pivot_data.round(2))\n",
    "else:\n",
    "    print(\"‚ùå Colonnes n√©cessaires pour la heatmap non trouv√©es\")"
   ]
