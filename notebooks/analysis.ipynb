    "    ax1.set_xlabel('Prix moyen (‚Ç¨)')\n",
    "    ax1.set_ylabel('Produit')\n",
    "    \n",
    "    # Produits les moins chers\n",
    "    ax2.barh(cheapest['product_clean'], cheapest['mean'], color='green', alpha=0.7)\n",
    "    ax2.set_title('Top 10 produits les moins chers')\n",
    "    ax2.set_xlabel('Prix moyen (‚Ç¨)')\n",
    "    ax2.set_ylabel('Produit')\n",
    "    \n",
    "    plt.tight_layout()\n",
    "    plt.show()\n",
    "    \n",
    "    print(\"üí∞ Produits les plus chers:\")\n",
    "    display(most_expensive[['product_clean', 'mean', 'count']].round(2))\n",
    "    \n",
    "    print(\"\\nüíö Produits les moins chers:\")\n",
    "    display(cheapest[['product_clean', 'mean', 'count']].round(2))\n",
    "else:\n",
    "    print(\"‚ùå Colonnes n√©cessaires non trouv√©es\")"
