   "source": [
    "## 3. Analyse par March√©"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "if not df.empty and 'market_clean' in df.columns and 'price' in df.columns:\n",
    "    # Prix moyens par march√©\n",
    "    market_stats = df.groupby('market_clean')['price'].agg(['mean', 'count', 'std']).reset_index()\n",
    "    market_stats = market_stats[market_stats['count'] >= 5]  # Filtre les march√©s avec peu de donn√©es\n",
    "    market_stats = market_stats.sort_values('mean', ascending=True)\n",
    "    \n",
    "    # Top 15 march√©s par prix moyen\n",
    "    top_markets = market_stats.tail(15)\n",
    "    \n",
    "    plt.figure(figsize=(12, 8))\n",
    "    plt.barh(top_markets['market_clean'], top_markets['mean'], color='lightcoral')\n",
    "    plt.title('Top 15 march√©s par prix moyen')\n",
    "    plt.xlabel('Prix moyen (‚Ç¨)')\n",
    "    plt.ylabel('March√©')\n",
    "    plt.tight_layout()\n",
    "    plt.show()\n",
    "    \n",
    "    # Statistiques des march√©s\n",
    "    print(\"üìä Statistiques des march√©s:\")\n",
    "    display(market_stats.describe())\n",
    "else:\n",
    "    print(\"‚ùå Colonnes n√©cessaires non trouv√©es\")"
   ]
