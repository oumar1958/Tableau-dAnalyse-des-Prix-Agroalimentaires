    "        # Prix moyens par origine\n",
    "        origin_stats = origin_data.groupby('origin')['price'].agg(['mean', 'count', 'std']).reset_index()\n",
    "        origin_stats = origin_stats[origin_stats['count'] >= 3]  # Filtre les origines avec peu de donn√©es\n",
    "        origin_stats = origin_stats.sort_values('mean', ascending=True)\n",
    "        \n",
    "        # Visualisation\n",
    "        plt.figure(figsize=(10, 6))\n",
    "        plt.barh(origin_stats['origin'], origin_stats['mean'], color='lightgreen')\n",
    "        plt.title('Prix moyens par pays d\'origine')\n",
    "        plt.xlabel('Prix moyen (‚Ç¨)')\n",
    "        plt.ylabel('Origine')\n",
    "        plt.tight_layout()\n",
    "        plt.show()\n",
    "        \n",
    "        print(\"üìä Statistiques par origine:\")\n",
    "        display(origin_stats)\n",
    "    else:\n",
    "        print(\"‚ùå Aucune donn√©e avec origine connue\")\n",
    "else:\n",
    "    print(\"‚ùå Colonnes 'origin' ou 'price' non trouv√©es\")"
