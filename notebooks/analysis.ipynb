    "        axes[1, 1].set_title('Prix moyen par cat√©gorie')\n",
    "        axes[1, 1].set_xlabel('Cat√©gorie')\n",
    "        axes[1, 1].set_ylabel('Prix moyen (‚Ç¨)')\n",
    "        axes[1, 1].tick_params(axis='x', rotation=45)\n",
    "    \n",
    "    plt.tight_layout()\n",
