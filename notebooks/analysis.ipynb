  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "if not df.empty and 'product_clean' in df.columns and 'price' in df.columns:\n",
    "    # Statistiques par produit\n",
    "    product_stats = df.groupby('product_clean')['price'].agg(['mean', 'count', 'std']).reset_index()\n",
    "    product_stats = product_stats[product_stats['count'] >= 3]  # Filtre les produits avec peu de donn√©es\n",
    "    \n",
